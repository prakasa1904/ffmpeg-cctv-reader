http:// {
	# header control
	header -X-Powered-By
	header Server "dPanel"

	handle {
		reverse_proxy localhost:8889
	}

	handle_errors {
		respond "{err.status_code} {err.status_text}"
	}

    log {
        output file /var/log/caddy/tunnel-proxy.access.log {
            roll_size 100mb
            roll_keep 3
            roll_keep_for 720h
        }
    }
}